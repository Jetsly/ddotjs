        "emitBOM": true,
        "experimentalDecorators": true,
        "moduleResolution":"node",
        "pretty": true,
        "declaration":true
    },
    "include": [
        "src/**/*.ts"
    ],
    "exclude": [
        "node_modules"
    ]
}